package java_programs;

class main{
	void print() {
		System.out.println("welcome to my home");
	}
}
interface F1{
	void add();
}
interface F2{
    void sub();
}
  class main_class extends main implements F2 ,F1{
	public void add() {
		System.out.println("Addition");
	}
	public void sub() {
		System.out.println("Subtraction");
	}
}

public class hybri_inheri_example {

	public static void main(String[] args) {
		main_class m=new main_class();
		m.print();
		m.add();
		m.sub();

	}

}
